dd = Dots({ scale:3})

p = Pluck ()
	.damping(0,3,1,6)
	.note.seq( [0,8,16,24], [1,1/2,1/3])	
	.pan.seq([-1,0,1],[1/5,1/6,1/2,1/7,1/23])
d = Delay(1/11,.4 )
p.fx.add( d )
p.amp(.1)

mr = Reverb({ roomsize:600, damping:.7 })
md = Delay(6000, .2)
Master.fx.add( md, mr)

